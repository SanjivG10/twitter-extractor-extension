{
    "manifest_version": 3,
    "name": "Webpyder",
    "description": "Web Spyder - Built to find me job",
    "version": "1.0",
    "action": {
      "default_popup": "popup/index.html",
      "default_icon": "webpyder.svg"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
        "matches": ["https://*.twitter.com/*"],
          "js": ["content/script.js"]
        }
      ],
    "permissions": [
        "tabs",
        "storage"
    ],
    "host_permissions": [
      "https://7dc0-2400-1a00-b060-314e-59ca-f99a-58e0-25a4.ngrok-free.app/*",
      "http://localhost:6000/*"
    ]
  }